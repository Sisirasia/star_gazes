def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    return True

def print_primes_less_than_100():
    for num in range(2, 100):
        if is_prime(num):
            print(num, end=" ")

# Call the function to print prime numbers less than 100
print_primes_less_than_100()
